{
  "expo": {
    "name": "EasyTop Up",
    "slug": "EasyTop Up",
    "version": "1.0.0",
    "jsEngine": "hermes",
    "experimental": {
      "newArchEnabled": true
    },
    "plugins" : ["react-native-reanimated/plugin"],

    "web": {
      // "favicon": "./assets/favicon.png"
    },

    "experiments": {
      "tsconfigPaths": true
    },

    "plugins": [],

    "orientation": "portrait",
    // "icon": "./assets/icon.png",

    "userInterfaceStyle": "light",

    "splash": {
      // "image": "./src/assets/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "useFrameworks" : "static",
      "newArchEnabled": true
    },
    "android": {
      "adaptiveIcon": {
        // "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
      },
      "newArchEnabled": true
    }
  }
}
